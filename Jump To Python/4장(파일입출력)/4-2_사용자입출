a=input() #input() 의 사용
print(a)

number=input("숫자를 입력하세요: ") #프롬프트를 띄워서 입력받기
print(number) 

a=123
print(a)
a="python"
print(a)
a=[1,2,3] #여러 자료형 출력가능
print(a)

print("life""is""too short") #큰따옴표로 둘러싸인 문자열은 + 연산과 동일
print("life" + "is" + "too short")

for i in range(10): 
    print(i,end='') #end를 이용해 한줄에 결과 출력
    
